        "for num in range(1042000,702648265):\n",
        "  sum1=0\n",
        "  no=num\n",
        "  order=len(str(num))\n",
        "  while num!=0:\n",
        "    rem=num%10\n",
        "    sum1=sum1+rem**order\n",
        "    num=int(num/10)\n",
        "  if no==sum1:\n",
        "    print(\"First ArmStrong Number\",no,\" \",sum1)\n",
        "    break"
      ],
